<div class="main users-new">
  <div class="container">
    <div class="form-heading">新規ユーザー登録</div>
    <div class="form users-form">
      <div class="form-body">
      <%= form_tag("/user_progates/create") do %>
        <p>ユーザー名</p>
        <!-- <input>タグを用いて1行の入力フォームを作成してください -->
        <input name="user_name" value="<%= @user.name %>">
        
        <p>メールアドレス</p>
        <!-- <input>タグを用いて1行の入力フォームを作成してください -->
        <input name="user_email" value="<%= @user.email %>">
        
        <input type="submit" value="新規登録">

      <% end %>
      </div>
    </div>
  </div>
</div>